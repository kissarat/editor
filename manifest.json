{
"manifest_version": 2,
"name": "HTML редактор",
"author": "Taras Labiak",
"description": "My first Chrome App",
"version": "0.1",
"app": {
	"background": {
			"scripts": ["background.js"]
	}
},
"icons": {
	"16": "/icon/logo16.png",
	"128": "/icon/logo128.png"
},
"kiosk_enabled": true,
"offline_enabled": true,
"permissions": [
	"app.runtime", "clipboardRead",
    {"fileSystem": ["write", "retainEntries", "directory"]},
	"storage"
]}
